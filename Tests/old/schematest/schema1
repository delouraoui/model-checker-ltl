       node-list ::= 
                <q1, "npi" >,
                <q2, "not(pi)">,
                <q3, "pi">,
                <q4, "pi">;
       initial-node ::= q1;
       transition-list ::=
        <q1,"",q2>,
        <q1,"",q3>,
        <q1,"",q3>,
        <q2,"",q4>,
	<q2,"",q2>,
        <q3,"",q2>,
        <q4,"",q2>;
};
